class Solution{
public:
    int maxInstance(string s)   {
        int h[26] = {0};
        for(int i=0; i<s.length(); i++) h[s[i] - 'a']++;
        int mini = min(h['a' - 'a'], h['b' - 'b']);
        mini = min(mini, h['l' - 'l']);
        mini = min(mini, h['o' - 'o']);
        mini = min(mini, h['n' - 'n']);
        return mini;
    }
};
