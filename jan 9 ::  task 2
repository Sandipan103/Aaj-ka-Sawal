private:

    bool isPrime(int n) {
        for(int i=2; i<n; i++)  {
            if(n % i == 0)  return false;
        }
        return true;
    }
    
public:

    int primeSum(int N){
        // code here
        int cnt = 0;
        while(N)    {
            int d = N % 10;
            if(isPrime(d))  cnt += d;
            N /= 10;
        }
        return cnt;
    }
